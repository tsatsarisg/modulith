{
    "name": "microservice",
    "version": "1.0.0",
    "description": "Example of an express microservice",
    "main": "src/index.ts",
    "scripts": {
        "clean": "rimraf dist",
        "build": "pnpm run clean && tsc --build tsconfig.build.json",
        "start": "pnpm run build && node dist/index.js",
        "dev": "ts-node-dev --exit-child --transpile-only src/index.ts",
        "test": "jest",
        "test:e2e": "jest test/e2e --verbose",
        "test:unit": "jest test/web --verbose",
        "lint": "eslint .",
        "lint:fix": "eslint . --fix",
        "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
        "format:check": "prettier --check \"src/**/*.ts\" \"test/**/*.ts\"",
        "docker:dev": "docker-compose -f deployment/docker-compose.yml --env-file deployment/envs/.env.dev up --build",
        "docker:prod": "docker-compose -f deployment/docker-compose.yml --env-file deployment/envs/.env.prod up --build",
        "docker:down": "docker-compose -f deployment/docker-compose.yml down"
    },
    "packageManager": "pnpm@10.28.0",
    "engines": {
        "node": ">=22.0.0",
        "pnpm": ">=10.28.0"
    },
    "keywords": [],
    "author": "George Tsatsaris",
    "license": "ISC",
    "devDependencies": {
        "@eslint/js": "^9.39.2",
        "@types/express": "^5.0.0",
        "@types/jest": "^29.5.14",
        "@types/node": "^22.10.0",
        "@types/supertest": "^6.0.3",
        "@typescript-eslint/eslint-plugin": "^8.18.0",
        "@typescript-eslint/parser": "^8.18.0",
        "eslint": "^9.17.0",
        "eslint-config-prettier": "^10.0.1",
        "eslint-import-resolver-typescript": "^3.7.0",
        "eslint-plugin-import": "^2.31.0",
        "eslint-plugin-jest": "^28.10.0",
        "eslint-plugin-n": "^17.15.0",
        "eslint-plugin-promise": "^7.2.1",
        "eslint-plugin-security": "^3.0.1",
        "jest": "^29.7.0",
        "mongodb-memory-server": "^11.0.1",
        "prettier": "^3.4.2",
        "rimraf": "^6.0.1",
        "supertest": "^7.2.2",
        "ts-jest": "^29.2.5",
        "ts-node-dev": "^2.0.0",
        "typescript": "^5.7.2",
        "typescript-eslint": "^8.53.0"
    },
    "dependencies": {
        "express": "^4.21.2",
        "helmet": "^8.0.0",
        "mongoose": "^9.1.4",
        "neverthrow": "^8.2.0",
        "zod": "^4.3.5"
    }
}
